{% block content %}
    <h1>Pridėk naują šalį!</h1>
    {{ form(form) }}
    <hr><br>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Pavadinimas</th>
            <th>Vėliava</th>
        </tr>
        {% for entity in entities %}
            <tr>

                <td>{{ entity.id }}</td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.flag }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}